<h1 class="text-center">Scores</h1>
<div class="container">
  <table class="table table-striped table-bordered">
    <tr>
      <td>Player ID</td>
      <td>Player</td>
      <td>DPS</td>
      <td>Job</td>
      <td>Duration</td>
      <td>Enemy</td>
      <td>Zone</td>
    </tr>
    <% @encounters.each do |encounter| %>
      <% encounter.scores.each do |score| %>
      <tr>
        <td><%= score.charaID %></td>
        <td><%= score.player %></td>
        <td><%= number_with_precision(score.dps, precision: 0) %></td>
        <td><%= score.jobString %></td>
        <td><%= encounter.durationS %></td>
        <td><%= encounter.enemy %></td>
        <td><%= encounter.zone %></td>
      </tr>
      <% end %>
    <% end %>
  </table>
</div>
